"VertexlitGeneric"
{
	"$basetexture" "rin/wheels_rav4/rim_black"
	"$bumpmap" "dannio/shared/bumpmap"
	"$normalmapalphaenvmapmask" 1
	"$blendtintbybasealpha" "1"
	"$nocull" "1"
	"$nodecal" "1"
   	$surfaceprop "metal"

 	$envmap "env_cubemap"
    	$envmaptint "[ .3 .3 .3]"
    	$envmapcontrast "1"
    	$envmapsaturation "1"
    	$envmapfresnel "1"

    	$phong "1"
    	$phongexponent "10"
    	$phongboost "1"
    	$phongfresnelranges "[ .7 .3 .01]"

    	"$colorfix" "[1 1 1]"
    	"Proxies"
    	{
       		 "Equals"
       		 {
           			 srcVar1 $colorfix
          			 resultVar    $color
       		 }
    	}
}

